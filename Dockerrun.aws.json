{
    "AWSEBDockerrunVersion": "1",
    "containerDefinitions": [
        {
            "name": "client",
            "image": "migzxc23/multi-client",
            "memory": 128,
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 3050,
                    "containerPort": 3050
                }
            ]
        },
        {
            "name": "server",
            "image": "migzxc23/multi-server",
            "memory": 128,
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8001,
                    "containerPort": 8001
                }
            ]
        },
        {
            "name": "worker",
            "image": "migzxc23/multi-worker",
            "memory": 128,
            "essential": true,
            "portMappings": []
        },
        {
            "name": "nginx",
            "image": "migzxc23/multi-nginx",
            "memory": 128,
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"]
        }
    ]
}
